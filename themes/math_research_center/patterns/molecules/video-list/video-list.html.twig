
{% set attributes = attributes.addClass('mrc-video-list') %}

<div{{ attributes }}>
    <div class="mrc-video-list__item">
        {% if video %}
            <div class="mrc-video-list__video">
                {{ video }}
            </div>
        {% endif %}
        <div class="mrc-video-list__content">
            {% if title %}
                <div class="mrc-video-list__title">{{ title }}</div>
            {% endif %}
        </div>
    </div>
</div>